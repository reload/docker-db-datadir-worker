# Dump is uploaded manually so we can generate the datadir whenever we want
00 0 * * * root (. /etc/container_environment.sh && cd /opt/datadir-preinit && ./preinit-tag.sh reload/helbredsprofilen:db-data reload/helbredsprofilen:db-datadir ../jobs/helbredsprofilen/datadir-preinit-sanitize.sql && FULL_DB_IMAGE=1 ./preinit-tag.sh reload/helbredsprofilen:db-data reload/helbredsprofilen:db-datadir ../jobs/helbredsprofilen/datadir-preinit-sanitize.sql) 2>&1 | /usr/bin/logger -t datadir-preinit:helbredsprofilen
